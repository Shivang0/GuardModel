{
  "name": "guardmodel",
  "version": "1.0.0",
  "description": "GitHub Action that scans ML model files for malicious code and security vulnerabilities",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:python": "pytest tests/",
    "test:all": "npm run test && npm run test:python",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/index.ts -o dist"
  },
  "keywords": [
    "github-action",
    "security",
    "ml",
    "machine-learning",
    "pickle",
    "pytorch",
    "keras",
    "onnx",
    "safetensors",
    "malware-detection"
  ],
  "author": "",
  "license": "ODbL-1.0",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@actions/artifact": "^2.1.0",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3",
    "js-yaml": "^4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.6",
    "@vercel/ncc": "^0.38.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
